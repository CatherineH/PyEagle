/* ULJunctionHandler.ulp generated on 17/17/13 13:16:03 */

#include "ULSupport.ulp"

string ULJunctionHandler(UL_JUNCTION Junction,string currentPath, string argument, char cacheMode)
{
	string baseName = GetFirstPathItem(currentPath);
	string nextPath = GetNextPath(currentPath);
	string baseType = GetFirstPathItem(nextPath);
	string cacheResult;

    if (cacheMode == CACHE_MODE_THIS)
	{
		cacheResult += intToStr(Junction.diameter) + "?";
		cacheResult += intToStr(Junction.x) + "?";
		cacheResult += intToStr(Junction.y) + "?";
		return cacheResult;
	}
	else if (baseName == "diameter")
		return intToStr(Junction.diameter);
	else if (baseName == "x")
		return intToStr(Junction.x);
	else if (baseName == "y")
		return intToStr(Junction.y);
		
	return "ERROR:" + intToStr(ERROR_JUNCTION_HANDLER);
}